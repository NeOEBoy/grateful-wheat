#!/usr/bin/env node
/**
 * Module dependencies.
 */
var app = require('../app');

function normalizePort(val) {
  var port = parseInt(val, 10);

  if (isNaN(port)) {
    // named pipe
    return val;
  }

  if (port >= 0) {
    // port number
    return port;
  }

  return false;
}

// 启动服务器
var port = normalizePort(process.env.PORT || '9001');
console.log('准备监听');
app.listen(port, () => {
  console.log('已启动监听，端口:' + port);
});
